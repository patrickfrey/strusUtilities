[Attribute]
    title = text content /doc/head/id();

[PatternLexem]
    word^0   = uc word //();
    romidx^0 = orig regex("\\b[IVX]+\\b") //();
    arbidx^0 = orig regex("\\b[0-9]+\\b") //();
    amount^1 = orig regex("\\b[1-9][0-9]*['][0-9]{3,3}\\b") //();
    amount^2 = orig regex("\\b[1-9][0-9]*['][0-9]{3,3}['][0-9]{3,3}\\b") //();

[PatternMatch std]
    bgepattern = "program_pp.rul";

[SearchIndex]
    ref <- bgepattern orig;


