    <entity lv="3"><pagelink id="Franklin Pierce"><text><E>Franklin Pierce</E></text>
        </pagelink>
      </entity>
    
    <br/>
    <text>from</text>
    <pagelink id="New Hampshire"><text><E>New Hampshire</E></text>
      </pagelink>
    </list>
  <list lv="l1"><text><E>Senator</E> 
</text>
    <br/>
    
